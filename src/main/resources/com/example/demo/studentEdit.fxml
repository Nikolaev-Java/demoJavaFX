<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="137.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.controllers.StudentEditController">
   <children>
      <HBox alignment="CENTER" focusTraversable="true" prefHeight="67.0" prefWidth="601.0" spacing="40.0" AnchorPane.bottomAnchor="70.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TextField fx:id="studentEditName" prefHeight="26.0" prefWidth="228.0" promptText="name" />
            <TextField fx:id="studentEditLastname" prefHeight="26.0" prefWidth="249.0" promptText="lastname" />
            <TextField fx:id="studentEditCourse" prefHeight="26.0" prefWidth="293.0" promptText="course" />
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutY="71.2" prefHeight="105.0" prefWidth="601.0" spacing="40.0" AnchorPane.bottomAnchor="-1.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="71.0">
         <children>
            <Button fx:id="studentEditOkBtn" mnemonicParsing="false" onAction="#studentEditOkBtnClicked" prefHeight="26.0" prefWidth="100.0" text="OK" />
            <Button fx:id="studentEditCancelBtn" mnemonicParsing="false" onAction="#studentEditCancelBtnClicked" prefWidth="100.0" text="Cancel" />
         </children>
      </HBox>
   </children>
</AnchorPane>
